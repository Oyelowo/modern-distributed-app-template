{
  "name": "app-web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite preview",
    "extract1": "formatjs extract 'src/**/*.{ts,tsx}' --additional-function-names='t' --ignore='**/*.d.ts'",
    "extract-compilez": "formatjs extract 'src/**/*.{ts,tsx}' --additional-function-names='t' --ignore='**/*.d.ts' --out-file temp.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]' && formatjs compile 'temp.json' --out-file lang/en.json && rm temp.json",
    "extract-compile-fr": "formatjs extract 'src/**/*.{ts,tsx}' --additional-function-names='t' --ignore='**/*.d.ts' --out-file temp.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]' && formatjs compile 'temp.json' --out-file lang/fr.json && rm temp.json",
    "com": "pnpm extract-compilez && pnpm extract-compile-fr",
    "compile": "formatjs extract 'src/**/*.{ts,tsx}' --additional-function-names='t' --ignore='**/*.d.ts' --out-file temp.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]' && formatjs compile 'temp.json' --out-file lang/fr.json --ast --out-file compiled-lang/fr.json && rm temp.json",
    "coma": "pnpm compile -- lang/fr.json --ast --out-file compiled-lang/fr.json --format formatter.js",
    "extract": "formatjs extract --additional-function-names='$t'",
    "extractx": "formatjs extract extract 'src/**/*.{ts,tsx}' --out-file lang.json --additional-function-names='$t'",
    "extract-compile": "formatjs extract './**/*.ts*' --out-file temp.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]' && formatjs compile 'temp.json' --out-file lang/en.json && rm temp.json",
    "compilez": "formatjs compile './**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "test": "playwright test --project=chromium",
    " //e2e:run": "runs test for multiple browaers: chrome, webkit and firefox",
    "e2e": "playwright test",
    "e2e:debug": "pnpm playwright test --project=chromium --debug"
  },
