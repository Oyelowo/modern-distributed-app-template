{
  "name": "oyelowo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npx turbo run build",
    "dev": "turbo run dev --parallel",
    "test:watch": "turbo run test --parallel",
    "test": "turbo run test:ci --parallel",
    "test-e2e": "turbo run cypress:run --parallel",
    "lint": "turbo run lint",
    "lint-fix": "turbo run lint-fix",
    "lint-check": "turbo run lint-check",
    "prettier-fix": "turbo run prettier-fix",
    "prettier-check": "turbo run prettier-check",
    "start": "next start",
    "generate": "turbo run generate --parallel",
    "generate-scoped": "turbo run generate --scope=@oyelowo/graphql-client",
    "prune": "turbo prune --scope=web"
  },
  "devDependencies": {
    "@turbo/codemod": "1.3.4",
    "prettier": "2.7.1",
    "turbo": "1.3.4"
  }
}
