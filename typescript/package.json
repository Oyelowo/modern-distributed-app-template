{
  "name": "oyelowo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npx turbo run build",
    "dev": "turbo run dev --parallel",
    "test:watch": "turbo run test --parallel",
    "test": "turbo run test:ci --parallel",
    "test-e2e": "turbo run cypress:run --parallel",
    "lint": "turbo run lint",
    "prettier": "turbo run prettier",
    "format": "prettier --write '**/*.{ts,tsx,.md,json}'",
    "start": "next start",
    "generate": "turbo run generate --scope=@oyelowo/graphql-client",
    "prune": "turbo prune --scope=web"
  },
  "devDependencies": {
    "@turbo/codemod": "1.2.16",
    "prettier": "2.6.2",
    "turbo": "1.2.16"
  },
  "dependencies": {
    "graphql-request": "4.3.0"
  }
}
