{
  "name": "oyelowo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test:watch": "turbo run test --parallel",
    "test": "turbo run test:ci --parallel",
    "lint": "turbo run lint",
    "prettier": "turbo run prettier",
    "format": "prettier --write '**/*.{ts,tsx,.md}'",
    "start": "next start",
    "prune": "turbo prune --scope=web"
  },
  "devDependencies": {
    "@turbo/codemod": "^1.1.0-canary.3",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "npm@8.1.2"
}
