{
  "name": "oyelowo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test --parallel",
    "test:ci": "turbo run test:ci --parallel",
    "lint": "turbo run lint",
    "prettier": "turbo run prettier",
    "format": "prettier --write '**/*.{ts,tsx,.md}'",
    "start": "next start",
    "prune": "turbo prune --scope=web"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "dist/**",
          ".next/**"
        ]
      },
      "test": {
        "outputs": []
      },
      "test:ci": {
        "outputs": []
      },
      "lint": {
        "outputs": []
      },
      "prettier": {
        "outputs": []
      },
      "dev": {
        "cache": false
      }
    }
  }
}