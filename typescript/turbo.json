{
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "public/dist/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["lint-check", "prettier-check", "build"]
    },
    "test:ci": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "cypress:run": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint-check": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint-fix": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "prettier-check": {
      "dependsOn": [],
      "outputs": []
    },
    "prettier-fix": {
      "dependsOn": [],
      "outputs": []
    },
    "install": {
      "dependsOn": [],
      "outputs": []
    },
    "upgrade": {
      "dependsOn": [],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "generate": {
      "cache": false
    }
  }
}
