{
  "name": "@oyelowo/graphql-client",
  "version": "1.0.0",
  "main": "./index.tsx",
  "types": "./index.tsx",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:old": "next build",
    "start": "next start",
    "lint-check": "next lint",
    "lint-fix": "next lint . --ext .js,.ts",
    "prettier-fix": "prettier --write .",
    "prettier-check": "prettier --check .",
    "compile": "tsc",
    "generate:old": "graphql-codegen --config codegen.yml",
    "generate": "node codegen.js",
    "generate:try": "tsc codegen.ts && node codegen.js && rm -rf ./codegen.js",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "prestart": "npm run generate",
    "predev": "npm run generate",
    "test": "jest --watch",
    "#test:ci": "jest --ci",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "upgrade": "npm update --save --force"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.0",
    "@graphql-codegen/cli": "^2.11.3",
    "@graphql-codegen/introspection": "^2.2.0",
    "@graphql-codegen/typescript": "^2.7.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@graphql-codegen/typescript-react-query": "^4.0.0",
    "@graphql-codegen/urql-introspection": "^2.2.0",
    "@tanstack/react-query": "^4.0.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.2",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.16",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.4",
    "eslint-plugin-testing-library": "^5.6.0",
    "fuse.js": "^6.6.2",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.13.0",
    "jest": "^28.1.3",
    "next": "^12.2.4",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
