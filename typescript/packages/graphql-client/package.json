{
  "name": "@oyelowo/graphql-client",
  "version": "1.0.0",
  "main": "./index.tsx",
  "types": "./index.tsx",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev:ui": "next dev",
    "dev": "export SET NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build": "tsc",
    "build:old": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "tsc",
    "generate:old": "graphql-codegen --config codegen.yml",
    "generate": "node codegen.js",
    "generate:try": "tsc codegen.ts && node codegen.js && rm -rf ./codegen.js",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "prestart": "npm run generate",
    "predev": "npm run generate",
    "test": "jest --watch",
    "#test:ci": "jest --ci",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "fuse.js": "6.6.2",
    "graphql-request": "4.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "next": "12.2.2",
    "graphql": "16.5.0",
    "react-query": "3.39.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.2.0",
    "@graphql-codegen/cli": "2.8.1",
    "@graphql-codegen/introspection": "2.2.0",
    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-operations": "2.5.1",
    "@graphql-codegen/typescript-react-query": "3.6.1",
    "@graphql-codegen/urql-introspection": "2.2.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.5",
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "eslint-plugin-testing-library": "5.5.1",
    "identity-obj-proxy": "3.0.0",
    "install": "0.13.0",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "react-is": "18.2.0",
    "react-test-renderer": "18.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
